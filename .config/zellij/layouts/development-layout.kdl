layout {
    default_tab_template {
        children
        pane size=1 borderless=true{
            plugin location="file:bare-bar.wasm"
        }
    }
    tab name="Nvim" focus=true{
        pane borderless=true
    }
    tab name="Git"{
        pane borderless=true
    }
    tab name="Terminal" split_direction="vertical"{
        pane name="term 1"
        pane name="term 2"
    }
  }
